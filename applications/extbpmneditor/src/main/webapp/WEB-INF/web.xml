<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>extbpmneditor</display-name>
    
    <servlet>
        <servlet-name>BpmnEditorServlet</servlet-name>
        <servlet-class>org.xsystem.bpmneditor.BpmnEditorRest</servlet-class>
        <!--servlet-class>org.xsystem.sqlhttp.PageServlet</servlet-class-->
        <init-param>
            <param-name>repository</param-name>
            <param-value>C:\REPO\GIT\ws7\applications\MalignantNeoplasmsPrevention\confmnp\src\main\webapp\bmpdbg\ </param-value>
        </init-param>
    </servlet>

    <servlet>
        <servlet-name>JMXConsoleRest</servlet-name>
        <servlet-class>org.xsystem.bpmneditor.JMXConsoleRest</servlet-class>
        <!--servlet-class>org.xsystem.sqlhttp.PageServlet</servlet-class-->
        
    </servlet>
  <!--  JMXConsoleRest    -->
  
    
    <servlet-mapping>
        <servlet-name>BpmnEditorServlet</servlet-name>
        <url-pattern>/rest/bpmneditor/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>JMXConsoleRest</servlet-name>
        <url-pattern>/jmxconsole/*</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
